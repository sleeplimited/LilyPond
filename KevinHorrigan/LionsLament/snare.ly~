\version "2.16.2"
snare = \markup { 
  \center-column {
    \combine
    \draw-line #'(0 . 3)
    \halign #CENTER 
    \override #'(box-padding . 0) \rounded-box {
      \draw-line #'(1 . 0.5) 
   }
 }
}
bassSnare = \markup {
  \center-column {
    \combine
    \draw-line #'(0 . 3)
    \halign #CENTER
    \override #'(box-padding . 0) 
    \rounded-box {
      \draw-line #'(1 . 0.5)
    }
    \combine
    \draw-line #'(0 . 1)
    \halign #CENTER
    \filled-box #'(0 . 0.5) #'(0 . 0.5) #0 
  }
}
\new TabStaff {
  \tabFullNotation
  c4 c4_\snare c4_\bassSnare c4 
}

