  \voiceOne
  \tabFullNotation

}
lower = {
  \key ees \major
  \voiceTwo
  \tabFullNotation

}
tab = {
  \key ees \major
  \numericTimeSignature
  \time 4/4
  \override Beam #'damping = #100000
  \tabFullNotation

}

